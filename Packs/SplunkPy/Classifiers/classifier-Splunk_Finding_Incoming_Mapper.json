                    "complex": {
                        "filters": [],
                        "root": "rule_title",
                        "transformers": [
                            {
                                "args": {
                                    "defaultValue": {
                                        "isContext": true,
                                        "value": {
                                            "simple": "orig_rule_title"
                                        }
                                    }
                                },
                                "operator": "SetIfEmpty"
                            }
                        ]
                    }
                },
                "Splunk Security Domain": {
                    "complex": {
                        "filters": [],
                        "root": "security_domain",
                        "transformers": [
                            {
                                "args": {
                                    "defaultValue": {
