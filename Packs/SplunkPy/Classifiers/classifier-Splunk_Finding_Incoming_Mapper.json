                    "complex": {
                        "filters": [],
                        "root": "drilldown_searches",
                        "transformers": [
                            {
                                "args": {
                                    "headers": {
                                        "isContext": false,
                                        "value": {
                                            "simple": "name,search"
                                        }
                                    }
                                },
                                "operator": "JsonToTable"
