                    "simple": "event_id"
                },
                "dbotMirrorInstance": {
                    "simple": "mirror_instance"
                },
                "dbotMirrorTags": {
                    "simple": "mirror_tags"
                },
                "details": {
                    "complex": {
                        "filters": [],
                        "root": "rule_description",
                        "transformers": [
                            {
                                "args": {
                                    "defaultValue": {
                                        "isContext": true,
                                        "value": {
                                            "simple": "orig_rule_description"
                                        }
                                    }
                                },
                                "operator": "SetIfEmpty"
                            },
                            {
                                "args": {
                                    "defaultValue": {
                                        "isContext": true,
                                        "value": {
                                            "simple": "status_description"
                                        }
                                    }
                                },
                                "operator": "SetIfEmpty"
                            }
                        ]
                    }
                },
                "Rule Name": {
                    "simple": "rule_name"
                },
