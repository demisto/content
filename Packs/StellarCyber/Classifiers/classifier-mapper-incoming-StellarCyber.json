{
    "defaultIncidentType": "Stellar Cyber Case",
    "description": "Maps incoming Stellar Cyber Cases to Cortex XSOAR incidents",
    "id": "Stellar Cyber - Incoming Mapper",
    "mapping": {
        "Stellar Cyber Case": {
            "dontMapEventToLabels": false,
            "internalMapping": {
                "name": {
                    "complex": {
                        "filters": [],
                        "root": "name",
                        "transformers": []
                    }
                },
                "Alert tags": {
                    "complex": {
                        "accessor": "tags",
                        "filters": [],
                        "root": "security_alerts.alert_metadata",
                        "transformers": []
                    }
                },
                "External Status": {
                    "complex": {
                        "filters": [],
                        "root": "status",
                        "transformers": []
                    }
                },
                "Number Of Found Related Alerts": {
                    "simple": "size"
                },
                "Process Names": {
                    "complex": {
                        "accessor": "detected_value",
                        "filters": [
                            [
                                {
                                    "left": {
                                        "isContext": true,
                                        "value": {
                                            "simple": "security_alerts.detected_field"
                                        }
                                    },
                                    "operator": "isEqualString",
                                    "right": {
                                        "isContext": false,
                                        "value": {
                                            "simple": "process_name"
                                        }
                                    }
                                }
                            ]
                        ],
                        "root": "security_alerts",
                        "transformers": [
                            {
                                "operator": "uniq"
                            }
                        ]
                    }
                },
                "Related Alerts": {
                    "complex": {
                        "filters": [],
                        "root": "security_alerts",
                        "transformers": [
                            {
                                "operator": "ConvertKeysToTableFieldFormat"
                            },
                            {
                                "args": {
                                    "headers": {
                                        "isContext": false,
                                        "value": {
                                            "simple": "displayname, description"
                                        }
                                    },
                                    "is_auto_json_transform": {
                                        "isContext": false
                                    },
                                    "json_transform_properties": {
                                        "isContext": false
                                    },
                                    "title": {
                                        "isContext": false
                                    }
                                },
                                "operator": "JsonToTable"
                            }
                        ]
                    }
                },
                "Stellar Cyber Case Alerts": {
                    "complex": {
                        "filters": [],
                        "root": "security_alerts",
                        "transformers": [
                            {
                                "operator": "ConvertKeysToTableFieldFormat"
                            }
                        ]
                    }
                },
                "Stellar Cyber Case ID": {
                    "simple": "_id"
                },
                "Stellar Cyber Case Score": {
                    "simple": "incident_score"
                },
                "Stellar Cyber Case URL": {
                    "simple": "case_url"
                },
                "Stellar Cyber Tenant ID": {
                    "simple": "cust_id"
                },
                "Tactic": {
                    "complex": {
                        "accessor": "xdr_tactic_name",
                        "filters": [],
                        "root": "security_alerts",
                        "transformers": [
                            {
                                "operator": "uniq"
                            }
                        ]
                    }
                },
                "Tactic ID": {
                    "complex": {
                        "accessor": "xdr_tactic_id",
                        "filters": [],
                        "root": "security_alerts",
                        "transformers": [
                            {
                                "operator": "uniq"
                            }
                        ]
                    }
                },
                "Technique": {
                    "complex": {
                        "accessor": "xdr_technique_name",
                        "filters": [],
                        "root": "security_alerts",
                        "transformers": [
                            {
                                "operator": "uniq"
                            }
                        ]
                    }
                },
                "Technique ID": {
                    "complex": {
                        "accessor": "xdr_technique_id",
                        "filters": [],
                        "root": "security_alerts",
                        "transformers": [
                            {
                                "operator": "uniq"
                            }
                        ]
                    }
                },
                "dbotMirrorDirection": {
                    "complex": {
                        "filters": [],
                        "root": "mirror_direction",
                        "transformers": []
                    }
                },
                "dbotMirrorId": {
                    "complex": {
                        "filters": [],
                        "root": "ticket_id",
                        "transformers": []
                    }
                },
                "dbotMirrorInstance": {
                    "complex": {
                        "filters": [],
                        "root": "mirror_instance",
                        "transformers": []
                    }
                },
                "details": {
                    "complex": {
                        "filters": [],
                        "root": "summary",
                        "transformers": []
                    }
                },
                "severity": {
                    "complex": {
                        "filters": [],
                        "root": "severity",
                        "transformers": []
                    }
                }
            }
        }
    },
    "name": "Stellar Cyber - Incoming Mapper",
    "type": "mapping-incoming",
    "version": -1,
    "fromVersion": "6.0.0"
}