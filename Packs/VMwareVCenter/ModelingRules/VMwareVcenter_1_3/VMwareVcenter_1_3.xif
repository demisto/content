// dnsmasq events 
call vmware_vcenter_virtualization_classification
| alter event_type = arrayindex(regextract(_raw_log, "^\<\d{1,3}\>\d+\s+\S+\s+\S+\s+(\S+)"), 0)
| filter event_type = "dnsmasq" and not is_virtualization
| alter 
