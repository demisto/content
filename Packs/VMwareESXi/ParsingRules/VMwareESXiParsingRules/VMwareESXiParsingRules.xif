[INGEST:vendor="VMware", product="ESXi", target_dataset="VMware_ESXi_raw", no_hit=drop]
alter date_part = arraystring(regextract(_raw_log, "(\d{4}\-\d{2}\-\d{2})T"), ""),
    time_part = arraystring(regextract(_raw_log, "T(\d{2}\:\d{2}\:\d{2})"), "")
| alter time_format = arraystring(arraycreate(date_part, time_part), " ")
| alter _time = parse_timestamp("%Y-%m-%d %H:%M:%S", time_format);