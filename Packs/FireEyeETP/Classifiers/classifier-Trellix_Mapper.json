{
    "description": "Trellix Mapper for alerts",
    "feed": false,
    "id": "TrellixMapper",
    "mapping": {
        "Trellix incident": {
			"dontMapEventToLabels": false,
			"internalMapping": {
                "Domain Name": {
					"simple": "domain"
				},
                "App message": {
					"simple": "msg"
				},
                "Traffic Direction": {
					"simple": "traffic_type"
				},
                "Verdict": {
					"simple": "verdict"
				},
                "External Start Time": {
					"simple": "alert_date"
				},
                "Vendor Product": {
					"simple": "product"
				},
                "occurred": {
                    "simple": "occurred"
                },
                "Alert Name": {
                    "simple": "name"
                },
                "UUID": {
                    "simple": "uuid"
                },
                "Alert Attack Time": {
					"simple": "attack-time"
				},
				"severity": {
					"simple": "severity"
				},
                "dbotMirrorDirection": {
                    "simple": "mirror_direction"
                },
                "dbotMirrorId": {
                    "simple": "id"
                },
                "dbotMirrorInstance": {
                    "simple": "mirror_instance"
                },
                "IncomingMirrorError": {
                    "simple": "in_mirror_error"
                }
            }
        }
    },
    "name": "TrellixMapper",
    "type": "mapping-incoming",
    "version": -1,
    "fromVersion": "6.5.0"
}