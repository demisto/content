[MODEL: dataset= "tigera_calico_raw"] 
alter 
     tigera_secure = arrayindex(regextract(_raw_log ,"({.*})"), 0)
| alter 
     action = tigera_secure -> action,
     ip_proto = tigera_secure -> proto,
     source_ip = tigera_secure -> source_ip,
     dest_ip = tigera_secure -> dest_ip
| alter
     xdm.source.ipv4 = if(tigera_secure ~= "(?:\d{1,3}\.){3}\d{1,3}", source_ip),
     xdm.source.ipv6 = if(tigera_secure ~= "(?:[a-fA-F\d]{0,4}\:){7}[\wa-fA-F]{0,4}", source_ip),
     xdm.source.host.hostname = tigera_secure -> source_name,
