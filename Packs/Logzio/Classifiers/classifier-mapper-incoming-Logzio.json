                },
                "Logz.io Alert Event ID": {
                    "complex": null,
                    "simple": "alertEventId"
                },
                "Logz.io Alert Summary": {
                    "complex": null,
                    "simple": "alertSummary"
                },
                "Logz.io Tags": {
                    "complex": null,
                    "simple": "tags"
                },
                "details": {
                    "complex": null,
                    "simple": "description"
                }
            }
        }
    },
    "version": -1,
    "fromVersion": "6.0.0"
}