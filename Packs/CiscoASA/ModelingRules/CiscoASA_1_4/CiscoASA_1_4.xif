    target_interface3 = arrayindex(regextract(_raw_log ,"from\s[A-Za-z0-9\-\_]+\:\d+\.\d+\.\d+\.\d+\sto\s([A-Za-z0-9\-\_]+)\:\d+\.\d+\.\d+\.\d+"),0),
