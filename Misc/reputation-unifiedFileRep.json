{
  "id": "unifiedFileRep",
  "version": -1,
  "regex": "\\b[a-fA-F\\d]{64}|[a-fA-F\\d]{40}|[a-fA-F\\d]{32}\\b",
  "details": "File",
  "reputationCommand": "file",
  "enhancementScriptNames": [
    "FileReputation",
    "SplunkSearch",
    "WildfireReport"
  ],
  "fromVersion": "5.0",
  "fileHashesPriority": [
    "md5",
    "sha1",
    "sha256"
  ],
  "defaultMapping": {
    "md5": {
      "simple": "File.MD5",
      "complex": null
    },
    "sha1": {
      "simple": "File.SHA1",
      "complex": null
    },
    "sha256": {
      "simple": "File.SHA256",
      "complex": null
    }
  }
}